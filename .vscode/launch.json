    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "prometheusEnabled": "false",
                "grafanaEnabled": "true",
                "jaegerEnabled": "true",
                "linkerdEnabled": "true",
                "rabbitMQEnabled": "true",
                "alertManagerEnabled": "true",
                "prometheusURL": "test.com",
                "grafanaURL": "test.com",
                "jaegerURL": "test.com",
                "linkerdURL": "https://www.test.com",
                "rabbitMQURL": "https://www.test.com",
                "alertManagerURL": "https://www.test.com",
                "logLevel": "debug",
                "port": "8089"
            }
        }
    ]
